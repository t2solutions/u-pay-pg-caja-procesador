  <xsd:complexType name="RecoveryData">
    <xsd:sequence>
      <xsd:element name="RecoveryPoint" type="xsd:string" minOccurs="0" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Punto de recuperacion del servicio en caso de una anterior caida</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
